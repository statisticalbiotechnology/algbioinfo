    "import os\n",
    "import os.path\n",
    "def count_files(path):\n",
    "    count = 0\n",
    "    for file in os.listdir(path):\n",
    "        if os.path.isdir(os.path.join(path,file)):\n",
    "            count += count_files(os.path.join(path,file))\n",
    "        count += 1\n",
    "    return count"
   ]
